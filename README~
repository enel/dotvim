[git add new Repo]
#git init
#git add .
#git commit -m "...."
#git remote add origin https://..../new.git
#git push origin master

[git add ALL data]
#git add .
#git commit -m "...."
#git push origin master

[Clone other's Repo]
git clone https://..../repo.git repo

[Creat git submodule]
#git submodule init
#git submodule update

[Install plug-in as a submodule]
#git submodule add https://...../plug-in.git bundle/plug-in
#git add .
#git commit -m "Install plug-in.vim bundle as a submodule."
#git push origin master


[submodule update]
#git submodule sync
#git submodule update
